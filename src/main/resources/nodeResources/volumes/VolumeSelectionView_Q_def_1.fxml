<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fend.session.node.volumes.VolumeSelectionController">
   <children>
      <HBox fx:id="HBox" fillHeight="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="259.0" style="-fx-background-color: transparent;">
         <children>
            <Button fx:id="selectVolumeButton" mnemonicParsing="false" onAction="#handleSelectVolumeButton" text="+V" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets bottom="10.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Label fx:id="volumePathLabel" maxWidth="-Infinity" prefHeight="25.0" prefWidth="127.0" text="volume path" textOverrun="CLIP" HBox.hgrow="ALWAYS">
               <padding>
                  <Insets left="2.0" top="10.0" />
               </padding>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
               <font>
                  <Font name="Monospaced Regular" size="12.0" />
               </font>
            </Label>
            <Button fx:id="showTable" disable="true" mnemonicParsing="false" onAction="#showTable" text="T" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="90.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font size="12.0" />
               </font>
            </Button>
            <Button fx:id="headerTableDisplayButton" disable="true" mnemonicParsing="false" onAction="#handleHeaderDisplayButton" text="H" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Monospaced Regular" size="12.0" />
               </font>
            </Button>
            <Button fx:id="qMatrixBtn" mnemonicParsing="false" onAction="#openQMatrix" text="[Q]">
               <font>
                  <Font name="Monospaced Regular" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <CheckBox fx:id="qcCheckBox" disable="true" mnemonicParsing="false" text="Q">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </CheckBox>
   </children>
</HBox>
