<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<?import org.controlsfx.control.CheckListView?>

<fx:root id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fend.session.node.qcTable.qcCheckBox.qcCheckListController">
    <stylesheets>
        <URL value="@/landingResources/landing.css" />
    </stylesheets>
    <children>
      <GridPane layoutX="68.0" layoutY="203.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="363.0" minHeight="10.0" prefHeight="363.0" vgrow="ALWAYS" />
            <RowConstraints maxHeight="194.0" minHeight="10.0" prefHeight="37.0" vgrow="NEVER" />
         </rowConstraints>
         <children>
            <CheckListView fx:id="checkListView" layoutX="-25.0" layoutY="-96.0" prefHeight="304.0" prefWidth="223.0"  GridPane.rowIndex="0" />
             
             
            <ButtonBar prefHeight="33.0" prefWidth="433.0" GridPane.rowIndex="1">
               <buttons>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleOnAdd" prefHeight="26.0" prefWidth="220.0" text="Add a type" />
              
                  <Button fx:id="doneButton" mnemonicParsing="false" onAction="#handleOnDone" text="Done" />
               </buttons>
            </ButtonBar>
         </children>
      </GridPane>
   </children>
</fx:root>
